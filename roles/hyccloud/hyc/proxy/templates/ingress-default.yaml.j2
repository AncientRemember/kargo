apiVersion: extensions/v1beta1
kind: Ingress
metadata:
  name: nginx-ingress
  annotations:
    ingress.kubernetes.io/proxy-body-size: "8m"
    ingress.kubernetes.io/rewrite-target: /
    ingress.kubernetes.io/ssl-redirect: "false"
spec:
  rules:
   - http:
      paths:
      - path: /hycindexsave
        backend:
          serviceName: hycindexsave
          servicePort: 8080
   - http:
      paths:
      - path: /sss
        backend:
          serviceName: sss
          servicePort: 8080
   - http:
      paths:
      - path: /das
        backend:
          serviceName: das
          servicePort: 8080
   - http:
      paths:
      - path: /groupservice
        backend:
          serviceName: groupservice
          servicePort: 8080        
